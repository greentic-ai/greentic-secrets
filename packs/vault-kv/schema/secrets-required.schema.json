{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Vault KV Provider Secrets",
  "type": "object",
  "properties": {
    "vault_token": { "type": "string", "x-secret": true },
    "vault_role_id": { "type": "string", "x-secret": true },
    "vault_secret_id": { "type": "string", "x-secret": true },
    "k8s_jwt": { "type": "string", "x-secret": true },
    "k8s_role": { "type": "string", "x-secret": true },
    "audit_sink_credentials": { "type": "string", "x-secret": true }
  },
  "required": ["audit_sink_credentials"],
  "additionalProperties": false
}
