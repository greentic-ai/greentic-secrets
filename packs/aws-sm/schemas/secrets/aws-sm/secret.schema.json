{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AWS Secrets Manager Provider Secrets",
  "type": "object",
  "properties": {
    "aws_access_key_id": { "type": "string", "x-secret": true },
    "aws_secret_access_key": { "type": "string", "x-secret": true },
    "aws_web_identity_token_file": { "type": "string", "x-secret": true },
    "audit_sink_credentials": { "type": "string", "x-secret": true }
  },
  "required": ["audit_sink_credentials"],
  "additionalProperties": false
}
