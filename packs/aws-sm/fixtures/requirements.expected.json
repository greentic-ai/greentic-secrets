{
  "provider_id": "greentic.secrets.aws-sm",
  "config": {
    "required": [
      "tenant_id",
      "environment",
      "region",
      "namespace_prefix",
      "audit",
      "timeouts",
      "retry_policy",
      "redaction_policy"
    ],
    "optional": [
      "assume_role_arn",
      "kms_key_id",
      "labels"
    ],
    "constraints": {
      "enum": {
        "environment": [
          "dev",
          "stage",
          "prod"
        ]
      }
    }
  },
  "secrets": {
    "required": [
      "audit_sink_credentials"
    ],
    "optional": [
      "aws_access_key_id",
      "aws_secret_access_key",
      "aws_web_identity_token_file"
    ],
    "constraints": {}
  },
  "capabilities": {
    "supports_read": true,
    "supports_write": true,
    "supports_delete": true
  },
  "setup_needs": {
    "public_base_url": false,
    "oauth": false,
    "subscriptions": false
  }
}
